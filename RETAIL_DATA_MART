## General options
[ident('Lookup_18'); jobmon_ident('Lookup_18')]
## Inputs
0< [] 'Lookup_15:DSLink33.v'
1< [] 'Prodcut_dim:DSLink34.v'
2< [] 'Transformer_50:DSLink64.v'
## Outputs
0> [modify (
  InvoiceDate:string[max=10]=InvoiceDate;)] 'Lookup_18:Target.v'
1> [] 'Lookup_18:DSLink55.v'
;

