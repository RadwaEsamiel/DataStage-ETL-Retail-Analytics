//
// Generated file to implement the V0S35_ACTIVATIONSALES_DATA_MART_Bonus_Transformer transform operator.
//

// define our input/output link names
inputname 0 Bonus;
outputname 0 Transformed;

initialize {
	// define our control variables
	int8 RowRejected0;
	int8 NullSetVar0;

}

mainloop {

	// initialise the rejected row variable
	RowRejected0 = 1;

	// evaluate columns (no constraints) for link: Transformed
	Transformed.Bonus = ((Bonus.SpendingScore * Bonus.AnnualIncomek) * 100);
	writerecord 0;
	RowRejected0 = 0;
}

finish {
}

